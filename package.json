  "scripts": {
    "start": "NODE_OPTIONS=\"--max-old-space-size=8192\" && storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build -o docs-build",
    "test-storybook": "test-storybook",
    "storybook:axe": "yarn build-storybook && yarn storybook:axeOnly",
    "storybook:axeOnly": "axe-storybook --build-dir docs-build",
    "test": "lerna run test -- --detectOpenHandles",
    "test:updateSnapshots": "lerna run test -- -u --detectOpenHandles",
    "namespace-check": "lerna run namespace-check",
    "prepare": "husky install",
    "lint": "lerna run lint",
    "build": "lerna run build --scope=@czi-sds/components && lerna run build --ignore=@czi-sds/components",
    "ci": "yarn install",
    "version:ci": "HUSKY=0 lerna version --yes --conventional-commits",
    "publish:ci": "HUSKY=0 lerna publish from-package --yes",
    "verdaccio:start": "npx verdaccio",
    "verdaccio:login": "npm adduser --registry http://localhost:4873",
    "publish:local": "npm adduser --registry http://localhost:4873 && lerna publish from-package --no-git-tag-version --no-push --registry http://localhost:4873 --yes",
    "test:publish": "echo 'Run yarn verdaccio:start in another terminal, then run yarn verdaccio:login to authenticate, then yarn publish:local'",
    "mcp:build": "yarn workspace @czi-sds/mcp build",
    "mcp:dev": "yarn workspace @czi-sds/mcp dev",
    "mcp:generate": "yarn workspace @czi-sds/mcp generate:all",
    "mcp:inspect": "yarn workspace @czi-sds/mcp inspect",
    "mcp:inspect:dev": "yarn workspace @czi-sds/mcp inspect:dev",
    "mcp:serve": "yarn workspace @czi-sds/mcp serve",
    "mcp:release": "yarn workspace @czi-sds/mcp release",
    "evaluate:code": "node scripts/evaluate-code/index.cjs",
    "evaluate:batch": "node scripts/evaluate-code/index.cjs --batch",
    "evaluate:test": "jest scripts/evaluate-code/evaluate.test.cjs"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,yml}": "npx prettier --write"
  },
  "files": [
    "dist"
  ],
  "description": "2021 Create-a-thon Science Initiative Component Library",
