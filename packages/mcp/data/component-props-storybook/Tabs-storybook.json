{
  "Tabs": {
    "props": {
      "items": {
        "type": "object",
        "isRequired": true,
        "description": "Here is an array of objects, each comprising a label and count for the tabs. Please note that this is specific to Storybook. In an actual implementation, it is recommended to pass the tabs as children of the Tabs component."
      },
      "sdsSize": {
        "type": "\"large\" | \"small\"",
        "isRequired": false,
        "description": "The size of the tabs",
        "options": [
          "large",
          "small"
        ]
      },
      "underlined": {
        "type": "boolean",
        "isRequired": false,
        "description": "Whether the tabs should be underlined"
      },
      "className": {
        "type": "string",
        "isRequired": false,
        "description": "Standard React className prop"
      },
      "style": {
        "type": "CSSProperties",
        "isRequired": false,
        "description": "Standard React style prop"
      },
      "children": {
        "type": "ReactNode",
        "isRequired": false,
        "description": "Standard React children prop"
      },
      "id": {
        "type": "string",
        "isRequired": false,
        "description": "Standard React id prop"
      }
    }
  }
}