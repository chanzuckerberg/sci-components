{
  "ButtonToggle": {
    "props": {
      "className": {
        "type": "string",
        "isRequired": false
      },
      "style": {
        "type": "CSSProperties",
        "isRequired": false
      },
      "children": {
        "type": "ReactNode",
        "isRequired": false,
        "description": "The content of the component."
      },
      "disabled": {
        "type": "\"false\" | \"true\"",
        "isRequired": false,
        "description": "If `true`, the component is disabled."
      },
      "color": {
        "type": "\"inherit\" | \"primary\" | \"secondary\" | \"success\" | \"error\" | \"info\" | \"warning\"",
        "isRequired": false,
        "description": "The color of the component.\nIt supports both default and custom theme colors, which can be added as shown in the\n[palette customization guide](https://mui.com/material-ui/customization/palette/#custom-colors).",
        "defaultValue": "primary"
      },
      "fullWidth": {
        "type": "\"false\" | \"true\"",
        "isRequired": false,
        "description": "If `true`, the button will take up the full width of its container.",
        "defaultValue": false
      },
      "href": {
        "type": "string",
        "isRequired": false,
        "description": "The URL to link to when the button is clicked.\nIf defined, an `a` element will be used as the root node."
      },
      "size": {
        "type": "\"small\" | \"medium\" | \"large\"",
        "isRequired": false,
        "description": "The size of the component.\n`small` is equivalent to the dense button styling.",
        "defaultValue": "medium"
      },
      "variant": {
        "type": "\"text\" | \"outlined\" | \"contained\"",
        "isRequired": false,
        "description": "The variant to use.",
        "defaultValue": "text"
      },
      "name": {
        "type": "string",
        "isRequired": false
      },
      "value": {
        "type": "\"string\" | \"number\" | \"readonly string[]\"",
        "isRequired": false
      },
      "id": {
        "type": "string",
        "isRequired": false
      },
      "onChange": {
        "type": "FormEventHandler<HTMLButtonElement>",
        "isRequired": false
      },
      "onClick": {
        "type": "function",
        "isRequired": false
      },
      "icon": {
        "type": "ReactElement",
        "isRequired": true
      },
      "sdsSize": {
        "type": "\"small\" | \"medium\" | \"large\"",
        "isRequired": false,
        "description": "The size of the component"
      },
      "sdsStage": {
        "type": "\"off\" | \"on\"",
        "isRequired": false
      },
      "sdsType": {
        "type": "\"primary\" | \"secondary\"",
        "isRequired": false,
        "description": "The type/variant of the component"
      }
    }
  }
}