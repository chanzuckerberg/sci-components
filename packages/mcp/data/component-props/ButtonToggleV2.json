{
  "ButtonToggleV2": {
    "props": {
      "backgroundOnHover": {
        "type": "\"false\" | \"true\"",
        "isRequired": false
      },
      "children": {
        "type": "ReactNode",
        "isRequired": false,
        "description": "The content of the component."
      },
      "disabled": {
        "type": "\"false\" | \"true\"",
        "isRequired": false,
        "description": "If `true`, the component is disabled.",
        "defaultValue": "false\nfalse"
      },
      "startIcon": {
        "type": "ReactElement",
        "isRequired": true,
        "description": "Element placed before the children."
      },
      "size": {
        "type": "\"small\" | \"medium\" | \"large\"",
        "isRequired": false,
        "description": "The size of the component.\n`small` is equivalent to the dense button styling.",
        "defaultValue": "medium"
      },
      "sdsType": {
        "type": "\"primary\" | \"secondary\" | \"destructive\"",
        "isRequired": false
      },
      "sdsStyle": {
        "type": "\"outline\" | \"solid\" | \"minimal\"",
        "isRequired": false
      },
      "sdsStage": {
        "type": "\"off\" | \"on\"",
        "isRequired": false
      }
    }
  }
}